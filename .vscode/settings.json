{
  "workbench.startupEditor": "none",
  "explorer.compactFolders": false,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "OM Theme (Default Dracula Italic)",
  "code-runner.executorMap": {
    "python": "clear ; python -u"
  },
  "code-runner.runInTerminal": true,
  "code-runner.ignoreSelection": true,
  "editor.fontFamily": "'Fira Code'",
  "terminal.integrated.fontFamily": "",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "emmet.includeLanguages": {
    "html": "html",
    "css": "css",
    "django-html": "html",
    "jinja-html": "html"
  },
  "html.format.indentInnerHtml": true,
  "html.format.indentHandlebars": true,
  "files.autoSave": "afterDelay",
  "notebook.formatOnSave.enabled": true,
  "[html]": {
    "editor.suggest.insertMode": "replace",
    "editor.formatOnType": true,
    "editor.tabSize": 2
  },
  "[django-html]": {
    "editor.autoIndent": "full",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.snippets.codeActions.enabled": true,
    "editor.suggest.showSnippets": true,
    "emmet.showAbbreviationSuggestions": true,
    "emmet.triggerExpansionOnTab": true,
    "editor.quickSuggestions": {
      "other": true,
      "comments": true,
      "strings": true
    }
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": true,
      "source.fixAll.unusedImports": true,
      "source.organizeImports": true
    }
  },
  "pylint.args": [
    "--disable=C0114",
    "--disable=C0115",
    "--disable=C0116",
    "--disable=E0611"
  ],
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": true,
  "python.analysis.diagnosticSeverityOverrides": {
    "reportUnknownMemberType": "none",
    "reportUnknownArgumentType": "none",
    "reportUnknownVariableType": "none",
    "reportUnknownLambdaType": "none",
    "reportUnknownParameterType": "none"
  },
  "python.analysis.typeCheckingMode": "basic",
  "black-formatter.showNotifications": "onError",
  "editor.wordWrap": "on",
  "workbench.editor.labelFormat": "short",
  "editor.guides.bracketPairs": true,
  "editor.mouseWheelZoom": true,
  "projectManager.openInNewWindowWhenClickingInStatusBar": true,
  "errorLens.gutterIconsEnabled": false,
  "errorLens.statusBarColorsEnabled": true,
  "errorLens.statusBarMessageEnabled": true,
  "errorLens.statusBarMessageType": "closestSeverity",
  "errorLens.followCursor": "closestProblem",
  "errorLens.scrollbarHackEnabled": true,
  "errorLens.fontStyleItalic": true,
  "errorLens.messageBackgroundMode": "message",
  "editor.linkedEditing": true,
  "conventionalCommits.lineBreak": "\\n",
  "eslint.lintTask.enable": true,
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "editor.fontLigatures": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "emmet.excludeLanguages": [],
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "scm.inputFontFamily": "'Fira Code'",
  "editor.tabSize": 2
}
